#!/usr/bin/env groovy

@Library('shared-library@main') _ //master or whatever branch

pipeline{

      agent any
        
        stages{

              stage('maven build'){
                  steps{
                      script{
		    	                sh '''
          cd Student
          mvn clean install
    '''
                      	  }
               	     }  
                 }	
                 
                 stage ('Check logs') {
                    steps {
                        filterLogs ('WARNING', 5)
                    }
                }
		
		
           }	       	     	         
}
